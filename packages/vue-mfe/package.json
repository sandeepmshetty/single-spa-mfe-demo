{
  "name": "@single-spa-demo/vue-mfe",
  "version": "1.0.0",
  "description": "Vue 3 micro-frontend for product catalog",
  "main": "src/main.ts",
  "scripts": {
    "start": "webpack serve --mode development --port 3002",
    "build": "webpack --mode production",
    "build:analyze": "webpack-bundle-analyzer dist/static/js/*.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .js,.vue,.ts",
    "lint:fix": "eslint src --ext .js,.vue,.ts --fix",
    "type-check": "vue-tsc --noEmit",
    "clean": "node ../../scripts/rm-paths.cjs dist",
    "serve": "serve dist -p 3002"
  },
  "dependencies": {
    "single-spa-vue": "^2.4.0",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitest/ui": "^0.34.6",
    "@vue/compiler-sfc": "^3.3.8",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.52.0",
    "eslint-plugin-vue": "^9.17.0",
    "html-webpack-plugin": "^5.6.4",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "serve": "^14.2.1",
    "style-loader": "^3.3.3",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "vue-loader": "^17.3.1",
    "vue-tsc": "^1.8.22",
    "webpack": "^5.102.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/single-spa-mfe-demo.git",
    "directory": "packages/vue-mfe"
  },
  "keywords": [
    "vue",
    "vue3",
    "single-spa",
    "micro-frontend",
    "product-catalog"
  ],
  "author": "Your Name",
  "license": "MIT"
}
