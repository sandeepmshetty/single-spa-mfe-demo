{
  "name": "@single-spa-demo/react-mfe",
  "version": "1.0.0",
  "description": "React micro-frontend for user management",
  "main": "src/index.tsx",
  "scripts": {
    "start": "webpack serve --mode development --port 3001",
    "build": "webpack --mode production",
    "build:analyze": "webpack-bundle-analyzer dist/static/js/*.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "node ../../scripts/utils/rm-paths.cjs dist",
    "serve": "serve dist -p 3001"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0",
    "@single-spa-demo/shared-library": "file:../shared-library",
    "styled-components": "^6.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/styled-components": "^5.1.28",
    "@types/node": "^20.8.7",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "html-webpack-plugin": "^5.5.3",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "babel-loader": "^9.1.3",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/user-event": "^14.5.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.6",
    "jest-environment-jsdom": "^29.7.0",
    "eslint": "^8.52.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "serve": "^14.2.1",
    "clean-webpack-plugin": "^4.0.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "repository": {
    "type": "git",
